
create TABLE geek
(
    id INT PRIMARY KEY NOT NULL,
    nom VARCHAR(100),
    prenom VARCHAR(100),
    sexe VARCHAR(255),
    mail VARCHAR(255),
   id_ci int
)
create TABLE centreInteret
(
    id INT PRIMARY KEY NOT NULL,
    CentreInteret VARCHAR(255)
)
ALTER TABLE geek
  ADD FOREIGN KEY (id_ci) REFERENCES centreInteret(id);
